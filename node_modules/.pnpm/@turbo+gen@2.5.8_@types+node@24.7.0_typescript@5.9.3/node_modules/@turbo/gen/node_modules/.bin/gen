#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/roberto.angulo/projects/personal/longevity-apps/node_modules/.pnpm/@turbo+gen@2.5.8_@types+node@24.7.0_typescript@5.9.3/node_modules/@turbo/gen/dist/node_modules:/Users/roberto.angulo/projects/personal/longevity-apps/node_modules/.pnpm/@turbo+gen@2.5.8_@types+node@24.7.0_typescript@5.9.3/node_modules/@turbo/gen/node_modules:/Users/roberto.angulo/projects/personal/longevity-apps/node_modules/.pnpm/@turbo+gen@2.5.8_@types+node@24.7.0_typescript@5.9.3/node_modules/@turbo/node_modules:/Users/roberto.angulo/projects/personal/longevity-apps/node_modules/.pnpm/@turbo+gen@2.5.8_@types+node@24.7.0_typescript@5.9.3/node_modules:/Users/roberto.angulo/projects/personal/longevity-apps/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/roberto.angulo/projects/personal/longevity-apps/node_modules/.pnpm/@turbo+gen@2.5.8_@types+node@24.7.0_typescript@5.9.3/node_modules/@turbo/gen/dist/node_modules:/Users/roberto.angulo/projects/personal/longevity-apps/node_modules/.pnpm/@turbo+gen@2.5.8_@types+node@24.7.0_typescript@5.9.3/node_modules/@turbo/gen/node_modules:/Users/roberto.angulo/projects/personal/longevity-apps/node_modules/.pnpm/@turbo+gen@2.5.8_@types+node@24.7.0_typescript@5.9.3/node_modules/@turbo/node_modules:/Users/roberto.angulo/projects/personal/longevity-apps/node_modules/.pnpm/@turbo+gen@2.5.8_@types+node@24.7.0_typescript@5.9.3/node_modules:/Users/roberto.angulo/projects/personal/longevity-apps/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../dist/cli.js" "$@"
else
  exec node  "$basedir/../../dist/cli.js" "$@"
fi
